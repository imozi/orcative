{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "include": ["src/**/*.ts", "src/**/*.vue", "src/**/*.md", "./.vitepress/**/*.ts", ".vitepress/config.ts"],
  "vueCompilerOptions": {
    "vitePressExtensions": [".md"]
  },
  "exclude": ["node_modules", "./.vitepress/cache", "./.vitepress/dist"]
}
